  "scripts": {
    "test": "karma start --no-single-run",
    "test:ne": "KARMA_SERVER=ne karma start",
    "test:types": "tsc --project test/types",
    "bundle": "webpack && webpack -p && yarn bundle:svg && node tsBannerGenerator.js",
    "bundle:svg": "node makesvg.js",
    "serve": "webpack-dev-server --inline --hot -d",
    "cpy-dist2doc": "mkdir -p doc/dist && cp -f -r dist doc",
    "doc": "jsdoc -c jsdoc.conf.json && yarn cpy-dist2doc",
    "tslint": "tslint index.d.ts",
    "rollup:build": "rollup -c",
    "rollup:watch": "rollup -c --watch",
    "postinstall": "npm install --no-save --no-optional fabric@^3.3.2"
  },
